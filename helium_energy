#=============================================================================
# Model description
# HTTU Experiment with nitrogen as a test coolant
# ------------------------------------------------------------------------------
# Champaign, IL
# Author: Velizar Zlatev 
#
# ==============================================================================
### Outlet pressure defined as hydrostatic


geometry_shift = 0.3
bed_height = 1.2
bed_radius = 0.85
bed_porosity = 0.39
inlet_temperature = 310 # 40 Celsius
heat_flux_left_wall = ${fparse 140000 / (1.2 * pi * 0.3)}

#heat_flux_left_wall = ${fparse 1 / (1.2 * pi * 0.3)}

initial_temperature = ${inlet_temperature} # chosen 30 Celsius
const_temp_right_wall = 300 # Kelvin 30 Celsius
dens = 0.166 # helium density 
output_pressure = 101000 # 101 kPa (abs)
initial_pressure = ${output_pressure} # 100 kPa (abs)

mass_flow_rate = 1.0
flow_area = '${fparse pi * ((geometry_shift + bed_radius) * (geometry_shift + bed_radius) - geometry_shift * geometry_shift)}'
flow_vel = '${fparse mass_flow_rate / flow_area / dens}'

pebble_diameter = 0.06

[Functions]
  [piecewise_func]
    type = ParsedFunction
    expression = 'if(t < t0, 0.0, if(t <= t0 + t1, c * (t - t0), ${heat_flux_left_wall}))'
    symbol_names = 't0 t1 c'
    symbol_values = '2 100 ${fparse heat_flux_left_wall/100}'
  []
[]

[Variables]
  [T_solid]
    type = INSFVEnergyVariable
    initial_condition = ${initial_temperature}
  []
[]

[Mesh]
  [gen]
    type = CartesianMeshGenerator
    dim = 2
    dx = '0.05 ${fparse bed_radius-2*.05} 0.05'
    ix = '20 10 20'
    dy = '${fparse bed_height/10} ${fparse bed_height*0.9}'
    iy = '20 60'
  []
  [shift]
    type = TransformGenerator
    input = gen
    transform = translate
    vector_value = '${geometry_shift} 0.0 0.0'
  []
  coord_type = RZ
[]


[FluidProperties]
  [fluid_properties_obj]
    type = HeliumFluidProperties # https://mooseframework.inl.gov/source/fluidproperties/NitrogenFluidProperties.html#lemmon2004
  []
[]


[Modules]
  [NavierStokesFV]
    # general control parameters
    compressibility = 'incompressible'
    porous_medium_treatment = true
    add_energy_equation = true
    
    # material property parameters
    density = rho
    dynamic_viscosity = mu
    thermal_conductivity = k
    specific_heat = cp
    
    # porous medium treatment parameters
    porosity = porosity
    # initial conditions
    initial_velocity = '0 ${flow_vel} 0'
    initial_pressure = '${initial_pressure}'
    initial_temperature = '${initial_temperature}'
    
    # boundary conditions
    inlet_boundaries = 'bottom'
    momentum_inlet_types = 'fixed-velocity'
    momentum_inlet_function = '0 ${flow_vel}'
    energy_inlet_types = 'fixed-temperature'
    energy_inlet_function = '${inlet_temperature}'
    
    #Wall Boundary Condition
    wall_boundaries = 'left right'
    momentum_wall_types = 'noslip noslip'
    energy_wall_types = 'heatflux fixed-temperature'
    energy_wall_function = 'piecewise_func ${const_temp_right_wall}'
    
    #Outlet Boundary Condition
    outlet_boundaries = 'top'
    momentum_outlet_types = 'fixed-pressure'
    pressure_function = '${output_pressure}'
    
    #Energy parameters 
    ambient_convection_alpha = 'alpha'
    ambient_temperature = 'T_solid'
  []
[]

[FVKernels]
   [energy_storage]
    type = PINSFVEnergyTimeDerivative
    variable = T_solid
    rho = rho_s
    cp = cp_s
    is_solid = true
    scaling = 1
    porosity = porosity
  []
  
  [solid_energy_diffusion]
    type = FVDiffusion
    coeff = k    # the same as the fluid TODO!! change to realisitic 
    variable = T_solid
  []
  
  [solid_energy_convection]
    type = PINSFVEnergyAmbientConvection
    variable = 'T_solid'
    is_solid = true
    T_fluid = 'T_fluid'
    T_solid = 'T_solid'
    h_solid_fluid = 'alpha'
  []
[]


[FunctorMaterials]
  [fluid_props_to_mat_props]
    type = GeneralFunctorFluidProps
    fp = fluid_properties_obj
    porosity = porosity
    pressure = pressure
    T_fluid = T_fluid
    speed = speed
    characteristic_length = ${pebble_diameter}
  []
  
  [generic_mat]
    type = ADGenericFunctorMaterial
    prop_names = 'rho_s  cp_s'
    prop_values = '2000  2000'
  []
  
   [alpha_mat]
    type = ADGenericFunctorMaterial
    prop_names = 'alpha'
    prop_values = '2e4'
  []
[]



[AuxVariables]
  [porosity]
    family = MONOMIAL
    order = CONSTANT
    fv = true
    initial_condition = ${bed_porosity}
  []
[]



[Executioner]
  type = Transient
  end_time = 1000
  [TimeStepper]
    type = IterationAdaptiveDT
    iteration_window = 2
    optimal_iterations = 8
    cutback_factor = 0.8
    growth_factor = 2
    dt = 1e-4
  []
  dtmax = 10
  line_search = l2
  solve_type = 'NEWTON'
  petsc_options_iname = '-pc_type -pc_factor_shift_type'
  petsc_options_value = 'lu NONZERO'
  nl_abs_tol = 5e-5
[]

[Postprocessors]
  [flux_computed_left]
  type = FunctionSideIntegral
  boundary = left
  function = piecewise_func
  []
[]

[Postprocessors]
  [flux_computed_right]
  type = SideDiffusiveFluxIntegral
  boundary = right
  variable = T_fluid
  functor_diffusivity = k
  []
  [flux_integral_left]
   type = SideDiffusiveFluxIntegral
  boundary = left
  variable = T_fluid
  functor_diffusivity = k 
  []
[]


[Postprocessors]
  [integral]
    type = SideAverageValue
    boundary = top
    variable = T_fluid
  []
[]


[Outputs]
  exodus = true
[] 
